<% @users.each do |u| %>
  <p><%= u.email %> 
  <p><%= u.fullname %> 
  <% if u.client %>
    <p><b>client</b>
  <% else %>
    <p><b>not a client</b>
  <% end %>
  <% if u.team %>
    <p><%= u.team.name %> 
  <% end %>
<p><%= link_to "Edit", edit_user_path(u), class: 'btn btn-primary' %>
<% end %>
